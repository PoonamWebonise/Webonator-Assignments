import java.lang.reflect.Constructor;
import java.lang.reflect.Method;


public class MyReflection {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Class <?> c=Demo.class;
		Method[] allMethods=c.getDeclaredMethods();
		for(Method eachMethod: allMethods)
		{
			System.out.println("method: "+eachMethod.getName());
		}
	
		Constructor<?> cons[]= c.getConstructors();
		for(Constructor eachConstructor: cons)
		{
			System.out.println("constructor: "+eachConstructor.getName());
		}
	}

}

class Demo {
	
	Demo() {
		
	}

	Demo(int a){
		
	}
	void print(){
		
	}
	
	void getValues(int a){
		
	}
	
	void getName(int x, int y){
		
	}
}